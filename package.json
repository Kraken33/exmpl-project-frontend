{
  "name": "exmple-project",
  "version": "0.0.2",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@loadable/component": "^5.11.0",
    "@reduxjs/toolkit": "^1.3.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/classnames": "^2.2.9",
    "@types/history": "^4.7.3",
    "@types/jest": "^26.0.15",
    "@types/loadable__component": "^5.10.0",
    "@types/lodash": "^4.14.164",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^12.12.22",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.15",
    "@types/react-input-mask": "^2.0.5",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.2",
    "@types/recompose": "^0.30.7",
    "@types/redux": "^3.6.0",
    "@types/redux-auth-wrapper": "^2.0.9",
    "@types/redux-form": "^8.2.0",
    "antd": "^4.7.3",
    "axios": "^0.19.1",
    "babel-plugin-import": "^1.13.0",
    "classnames": "^2.2.6",
    "codemirror": "^5.58.2",
    "connected-react-router": "^6.6.1",
    "cross-env": "^6.0.3",
    "customize-cra": "^0.9.1",
    "dayjs": "^1.9.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "history": "^4.10.1",
    "immer": "^8.0.0",
    "lodash": "^4.17.20",
    "nanoid": "^2.1.9",
    "node-sass": "^4.14.1",
    "p-min-delay": "^3.1.0",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-json-view": "^1.19.1",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-breadcrumbs-hoc": "^3.2.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^8.2.6",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:no-interactive": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "server": "serve -s build -l 4000",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "prettier": "prettier --write src/**/*.ts{,x}",
    "_docker:compiler": "APP_NAME=app; docker build -t $APP_NAME . --build-arg APP_NAME=$APP_NAME; docker run -d --name $APP_NAME -p 80:80 $APP_NAME"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-typescript",
      "airbnb/hooks",
      "plugin:@typescript-eslint/recommended",
      "plugin:jest/recommended",
      "prettier",
      "prettier/react",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "react",
      "@typescript-eslint",
      "jest"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "rules": {
      "linebreak-style": "off",
      "import/prefer-default-export": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-unused-expressions": "off",
      "@typescript-eslint/ban-types": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/no-shadow": "off",
      "react/jsx-props-no-spreading": "off",
      "func-names": "off",
      "no-param-reassign": "off",
      "react/prop-types": "off",
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "circular-dependency-plugin": "^5.2.2",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "4.0.0",
    "eslint-plugin-unused-imports": "^0.1.2",
    "husky": "^4.2.3",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^10.0.9",
    "prettier": "^2.1.2",
    "prettier-plugin-import-sort": "^0.0.6",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^26.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test:no-interactive"
    }
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
